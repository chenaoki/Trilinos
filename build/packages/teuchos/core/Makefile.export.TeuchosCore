## ---------------------------------------------------------------------------
## Compilers used by Trilinos/TeuchosCore build
## ---------------------------------------------------------------------------

TeuchosCore_CXX_COMPILER=/usr/bin/mpicxx

TeuchosCore_C_COMPILER=/usr/bin/mpicc

TeuchosCore_FORTRAN_COMPILER=/usr/bin/mpif90
TeuchosCore_Fortran_COMPILER=/usr/bin/mpif90


## ---------------------------------------------------------------------------
## Compiler flags used by Trilinos/TeuchosCore build
## ---------------------------------------------------------------------------

## Set compiler flags, including those determined by build type
TeuchosCore_CXX_FLAGS= -std=c++11 -O3
TeuchosCore_CXX_COMPILER_FLAGS= -std=c++11 -O3

TeuchosCore_C_FLAGS= -O3
TeuchosCore_C_COMPILER_FLAGS= -O3

TeuchosCore_FORTRAN_FLAGS= -O3
TeuchosCore_Fortran_COMPILER_FLAGS= -O3

## Extra link flags (e.g., specification of fortran libraries)
TeuchosCore_EXTRA_LD_FLAGS=-lpthread

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
TeuchosCore_SHARED_LIB_RPATH_COMMAND=
TeuchosCore_BUILD_SHARED_LIBS=OFF

TeuchosCore_LINKER=/usr/bin/ld
TeuchosCore_AR=/usr/bin/ar

## ---------------------------------------------------------------------------
## Set library specifications and paths 
## ---------------------------------------------------------------------------

## The project version number
TeuchosCore_VERSION=12.4.2

## List of package include dirs
TeuchosCore_INCLUDE_DIRS= -I/home/tomii/simenv/trilinos/build -I/home/tomii/simenv/trilinos/build/packages/teuchos/core/src -I/home/tomii/simenv/trilinos/packages/teuchos/core/src -I/home/tomii/simenv/trilinos/build/packages/kokkos/core/src -I/home/tomii/simenv/trilinos/packages/kokkos/core/src

## List of package library paths
TeuchosCore_LIBRARY_DIRS= -L/home/tomii/simenv/trilinos/build/packages/teuchos/core/src -L/home/tomii/simenv/trilinos/build/packages/kokkos/core/src

## List of package libraries
TeuchosCore_LIBRARIES= -lteuchoscore -lkokkoscore

## Specification of directories for TPL headers
TeuchosCore_TPL_INCLUDE_DIRS=

## Specification of directories for TPL libraries
TeuchosCore_TPL_LIBRARY_DIRS=

## List of required TPLs
TeuchosCore_TPL_LIBRARIES=

## ---------------------------------------------------------------------------
## MPI specific variables
##   These variables are provided to make it easier to get the mpi libraries
##   and includes on systems that do not use the mpi wrappers for compiling
## ---------------------------------------------------------------------------

TeuchosCore_MPI_LIBRARIES=
TeuchosCore_MPI_LIBRARY_DIRS=
TeuchosCore_MPI_INCLUDE_DIRS=
TeuchosCore_MPI_EXEC=/usr/bin/mpiexec
TeuchosCore_MPI_EXEC_MAX_NUMPROCS=4
TeuchosCore_MPI_EXEC_NUMPROCS_FLAG=-np

## ---------------------------------------------------------------------------
## Set useful general variables 
## ---------------------------------------------------------------------------

## The packages enabled for this project
TeuchosCore_PACKAGE_LIST= TeuchosCore KokkosCore

## The TPLs enabled for this project
TeuchosCore_TPL_LIST= MPI Pthread

