##############################################################################
#
# Make variables for use by Trilinos/Epetra clients. 
#
# Do not edit: This file was generated automatically by CMake.
#
##############################################################################


## ---------------------------------------------------------------------------
## Compilers used by Trilinos/Epetra build
## ---------------------------------------------------------------------------

Epetra_CXX_COMPILER=/usr/bin/mpicxx

Epetra_C_COMPILER=/usr/bin/mpicc

Epetra_FORTRAN_COMPILER=/usr/bin/mpif90
Epetra_Fortran_COMPILER=/usr/bin/mpif90


## ---------------------------------------------------------------------------
## Compiler flags used by Trilinos/Epetra build
## ---------------------------------------------------------------------------

## Set compiler flags, including those determined by build type
Epetra_CXX_FLAGS= -std=c++11 -O3
Epetra_CXX_COMPILER_FLAGS= -std=c++11 -O3

Epetra_C_FLAGS= -O3
Epetra_C_COMPILER_FLAGS= -O3

Epetra_FORTRAN_FLAGS= -O3
Epetra_Fortran_COMPILER_FLAGS= -O3

## Extra link flags (e.g., specification of fortran libraries)
Epetra_EXTRA_LD_FLAGS=-lpthread

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
Epetra_SHARED_LIB_RPATH_COMMAND=
Epetra_BUILD_SHARED_LIBS=OFF

Epetra_LINKER=/usr/bin/ld
Epetra_AR=/usr/bin/ar

## ---------------------------------------------------------------------------
## Set library specifications and paths 
## ---------------------------------------------------------------------------

## The project version number
Epetra_VERSION=12.4.2

## List of package include dirs
Epetra_INCLUDE_DIRS= -I../install/include

## List of package library paths
Epetra_LIBRARY_DIRS= -L../install/lib

## List of package libraries
Epetra_LIBRARIES= -lepetra -lteuchoskokkoscomm -lteuchoskokkoscompat -lteuchosremainder -lteuchosnumerics -lteuchoscomm -lteuchosparameterlist -lteuchoscore -lkokkoscore

## Specification of directories for TPL headers
Epetra_TPL_INCLUDE_DIRS= -I/usr/include -I/usr/include

## Specification of directories for TPL libraries
Epetra_TPL_LIBRARY_DIRS=

## List of required TPLs
Epetra_TPL_LIBRARIES= /usr/lib/liblapack.so /usr/lib/libblas.so

## ---------------------------------------------------------------------------
## MPI specific variables
##   These variables are provided to make it easier to get the mpi libraries
##   and includes on systems that do not use the mpi wrappers for compiling
## ---------------------------------------------------------------------------

Epetra_MPI_LIBRARIES=
Epetra_MPI_LIBRARY_DIRS=
Epetra_MPI_INCLUDE_DIRS=
Epetra_MPI_EXEC=/usr/bin/mpiexec
Epetra_MPI_EXEC_MAX_NUMPROCS=4
Epetra_MPI_EXEC_NUMPROCS_FLAG=-np

## ---------------------------------------------------------------------------
## Set useful general variables 
## ---------------------------------------------------------------------------

## The packages enabled for this project
Epetra_PACKAGE_LIST= Epetra Teuchos TeuchosKokkosComm TeuchosKokkosCompat TeuchosRemainder TeuchosNumerics TeuchosComm TeuchosParameterList TeuchosCore KokkosCore

## The TPLs enabled for this project
Epetra_TPL_LIST= LAPACK BLAS MPI Pthread

## ---------------------------------------------------------------------------
## Deprecated variables
##   These variables from the autotools version of Makefile.export.<package>
##   have been deprecated. To help avoid confusion with projects that haven't
##   migrated to the new version of the variable names when the don't compile.
##   This will put an obvious statement in the link line that these Trilinos
##   Makefile variables are no longer used. Without this there would be a link
##   error with no obvious reason as to why other than the lack of Trilinos libs
##   which would be confusing since the link line in the Makefile would have 
##   stated to put them on the line.
## ---------------------------------------------------------------------------

EPETRA_LIBS="The Trilinos variable EPETRA_LIBS has been deprecated. Please see the documentation on the Makefile.export capability for instructions on how to transition to the new system."
EPETRA_INCLUDES="The Trilinos variable EPETRA_INCLUDES has been deprecated. Please see the documentation on the Makefile.export capability for instructions on how to transition to the new system."


